{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "src/app/**/*.{ts,tsx}",
        "tests/**/*.test.{ts,js}"
    ],
    "project": ["src/**/*.{ts,tsx,js,jsx}", "scripts/**/*.{ts,tsx,js,jsx}"],
    "ignore": ["**/*.config.*"],
    "ignoreDependencies": ["eslint-nibble", "tailwindcss", "source-map-js"],
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "tags": ["-lintignore"]
}
